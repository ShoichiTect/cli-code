{
	"name": "cli-code",
	"version": "1.0.2",
	"license": "MIT",
	"bin": {
		"cli": "dist/core/main.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "eslint . --ext .ts,.tsx,.js,.cjs,.mjs",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.cjs,.mjs --fix",
		"format": "prettier . --write",
		"test": "ava",
		"test:coverage": "c8 --reporter=text --reporter=html ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@anthropic-ai/sdk": "^0.70.1",
		"@google/genai": "^1.30.0",
		"chalk": "^5.4.1",
		"commander": "^14.0.0",
		"groq-sdk": "^0.27.0",
		"https-proxy-agent": "^7.0.6",
		"socks-proxy-agent": "^8.0.5"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"c8": "^10.1.3",
		"chalk": "^5.2.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.3"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		],
		"files": [
			"test/**/*.test.ts",
			"!**/*.d.ts"
		]
	},
	"prettier": "@vdemedes/prettier-config"
}
